{{each prdls}}
<li class="prd-item" data-id="{{$value.prd_id}}">
    <div class="prd-img">
        <img src="{{$value.prdImg_url}}" alt="">
    </div>
    <div class="prd-desc">
        <div class="p-title">{{$value.prd_name}}</div>
        <div class="p-price">
            <span class="p-salep">{{$value.prd_saleprice}}</span>
            <span class="p-lowp">{{$value.prd_price}}</span>
        </div>
        <div class="p-like">{{$value.prd_like}}</div>
    </div>
</li>
{{/each}}